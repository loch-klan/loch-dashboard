    "build": "sh -ac '. ./src/env/.${ENV}; react-scripts build'",
    "build:dev": "ENV=development npm run build",
    "predeploy:dev": "npm run build:dev",
    "deploy:dev": "aws s3 sync build/ s3://staging.loch.com",
